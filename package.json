{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --color --progress --mode development -d",
    "server": "nodemon -r esm --watch src/server --inspect src/server/index.js",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "heroku-preinstall": "npm install -g webpack",
    "heroku-postinstall": "npm install --dev && webpack -p",
    "test": "mocha ./test/**/*.test.js"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "@ckeditor/ckeditor5-build-inline": "^11.1.1",
    "@ckeditor/ckeditor5-react": "^1.0.0",
    "amazon-cognito-identity-js": "^3.0.3",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.2",
    "cognito-express": "^2.0.12",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.6.0",
    "directory-named-webpack-plugin": "^4.0.0",
    "dotenv": "^6.1.0",
    "esm": "^3.0.84",
    "express": "^4.16.3",
    "log4js": "^3.0.6",
    "mongodb": "^3.1.9",
    "mongoose": "^5.3.12",
    "morgan": "^1.9.1",
    "node-jose": "^1.1.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
